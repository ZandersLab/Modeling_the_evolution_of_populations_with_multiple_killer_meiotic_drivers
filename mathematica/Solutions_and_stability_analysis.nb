  RowBox[{
   RowBox[{"t", "[", "1", "]"}], "+", 
   RowBox[{"t", "[", "2", "]"}]}], 
  RowBox[{
   RowBox[{"t", "[", "1", "]"}], "+", 
   RowBox[{"t", "[", "2", "]"}], "-", 
