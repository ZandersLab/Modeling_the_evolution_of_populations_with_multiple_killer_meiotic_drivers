Cell[TextData[{
 "The unstable solutions obtained from using eq(2)  indicate that solution \
can be:\n ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    RowBox[{"x", "[", "1", "]"}], "\[Rule]", "0"}], ",", 
   RowBox[{
    RowBox[{"x", "[", "2", "]"}], "\[Rule]", 
    FractionBox["ty", 
     RowBox[{"tx", "+", "ty"}]]}], ",", 
   RowBox[{
    RowBox[{"x", "[", "3", "]"}], "\[Rule]", 
    FractionBox["tx", 
     RowBox[{"tx", "+", "ty"}]]}]}]],
  CellChangeTimes->{
   3.901540944795074*^9, 3.901631753990367*^9, {3.9016319021445217`*^9, 
    3.901631903620667*^9}},ExpressionUUID->
  "0dd2bd21-61fd-4e84-9927-ce6342a5c792"],
 "\n ",
